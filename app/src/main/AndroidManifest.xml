<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.wifindus.meshclient" >

<application
    android:allowBackup="true"
    android:icon="@drawable/mesh_blue"
    android:label="@string/app_name"
    android:theme="@style/AppTheme" >
    <activity
        android:name=".MainActivity"
        android:label="@string/app_name" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <activity
        android:name=".MyDeviceActivity"
        android:label="My Device" >
    </activity>

    <activity
        android:name=".SelectUserActivity"
        android:label="Select User" >
    </activity>

    <activity
        android:name=".MyIncidentsActivity"
        android:label="My Incidents" >
    </activity>

    <activity
        android:name=".MapActivity"
        android:label="Map" >
    </activity>

    <activity
        android:name=".PeopleActivity"
        android:label="People" >
    </activity>

    <activity
        android:name=".LocationsActivity"
        android:label="Locations" >
    </activity>

    <activity
        android:name=".NodesActivity"
        android:label="Nodes" >
    </activity>

    <activity
        android:name=".LogActivity"
        android:label="Log" >
    </activity>

    <activity
        android:name=".AboutAppActivity"
        android:label="About App" >
    </activity>

</application>

</manifest>
